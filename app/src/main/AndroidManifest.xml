<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.afinal">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity android:name=".AboutUsActivity"
            android:parentActivityName=".MainActivity" ></activity>
        <activity
            android:name=".SonghuaActivity"
            android:parentActivityName=".CoachActivity" />
        <activity
            android:name=".ChibingActivity"
            android:parentActivityName=".CoachActivity" />
        <activity
            android:name=".JieXiaoActivity"
            android:parentActivityName=".CoachActivity" />
        <activity
            android:name=".BadIntroActivity"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".DisplayerActivity"
            android:parentActivityName=".TutorialActivity" />
        <activity
            android:name=".WebActivity"
            android:parentActivityName=".TournamentActivity" />
        <activity
            android:name=".TournamentActivity"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".CoachActivity"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".TutorialActivity"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".GetReportActivity"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".RequestAdvice"
            android:parentActivityName=".MainActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <service android:name=".services.MyFirebaseInstanceService">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT" />
            </intent-filter>
        </service>

    </application>

</manifest>